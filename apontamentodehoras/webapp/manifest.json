{
  "_version": "1.28.0",
  "sap.app": {
    "id": "apontamento.apontamentodehoras",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:worklist",
      "version": "1.7.4",
      "toolsId": "aefcab27-1557-4449-803f-8fcadc603a17"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZAPONTAMENTO_FB_2_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.84.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "apontamento.apontamentodehoras.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "apontamento.apontamentodehoras.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [

        {
          "pattern": "",
          "name": "login",
          "target": [
            "login"
          ]
        },

        {
          "pattern": "Home",
          "name": "home",
          "target": [
            "home"
          ]
        },

        {
          "pattern": "Apontamento",
          "name": "apontamento",
          "target": [
            "apontamento"
          ]
        },

        {
          "pattern": "newClient",
          "name": "newclient",
          "target": [
            "newclient"
          ]
        },

        {
          "pattern": "newEmployee",
          "name": "newemployee",
          "target": [
            "newemployee"
          ]
        },

       
        {
          "pattern": "",
          "name": "worklist",
          "target": [
            "worklist"
          ]
        },
        {
          "pattern": "cliente/{objectId}",
          "name": "object",
          "target": [
            "object"
          ]
        },
        {
          "pattern": "create",
          "name": "create",
          "target": [
            "create"
          ]
        },
        {
          "pattern": "horas/{objectId}",
          "name": "horas",
          "target": [
            "horas"
          ]
        }
      ],
      "targets": {

        "login": {
          "viewName": "Login",
          "viewId": "login",
          "viewLevel": 1,
          "title": "{i18n>worklistViewTitle}"
        }, 

        "home": {
          "viewName": "Home",
          "viewId": "home",
          "viewLevel": 2,
          "title": "{i18n>worklistViewTitle}"
        }, 

        "newclient": {
          "viewName": "NewClient",
          "viewId": "newclient",
          "viewLevel": 3,
          "title": "{i18n>worklistViewTitle}"
        }, 

        "newemployee": {
          "viewName": "NewEmployee",
          "viewId": "newemployee",
          "viewLevel": 3,
          "title": "{i18n>worklistViewTitle}"
        }, 

        "apontamento": {
          "viewName": "Apontamento",
          "viewId": "apontamento",
          "viewLevel": 3,
          "title": "{i18n>worklistViewTitle}"
        },
        
        "worklist": {
          "viewName": "Worklist",
          "viewId": "worklist",
          "viewLevel": 3,
          "title": "{i18n>worklistViewTitle}"
        },
        "object": {
          "viewName": "Object",
          "viewId": "object",
          "viewLevel": 2,
          "title": "{i18n>objectViewTitle}"
        },
        "objectNotFound": {
          "viewName": "ObjectNotFound",
          "viewId": "objectNotFound"
        },
        "create": {
          "viewId": "create",
          "viewName": "Create",
          "viewLevel": 2
        },
        "horas": {
          "viewId": "horas",
          "viewName": "Horas",
          "viewLevel": 3
        },
        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound"
        }
      }
    },
    "rootView": {
      "viewName": "apontamento.apontamentodehoras.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    }
  }
}